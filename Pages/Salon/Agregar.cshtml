@page
@model pro.Pages.Salon.AgregarModel
@{
    ViewData["Title"] = "Agregar";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}
@section Scripts{
    <script src="~/js/AgregarSalon.js" type="text/javascript"></script>
}
<h2>Agregar</h2>

<div class="col-md-12">
    <form id="formMaestro" method="post">
        <div class="col-md-10">
            <div class="row">
                <div class="col-md-1">
                    <label for="inputTitulo">Salon</label>
                </div>
                <div class="col-md-11">
                    <input type="text" id="txtSalon" class="form-control" />
                </div>
            </div>
        </div>
    </form>
    <div class="col-md-12 text-right">
        <button id="btnagregar" class="btn btn-info" onclick="agregarSalon()">Agregar</button>
    </div>
</div>
